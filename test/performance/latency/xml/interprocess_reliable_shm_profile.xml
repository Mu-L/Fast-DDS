                <durability>
                    <kind>VOLATILE</kind>
                </durability>
                <data_sharing>
                    <kind>OFF</kind>
                </data_sharing>
            </qos>
            <times>
                <!-- <initial_acknack_delay>
                    <sec>1</sec>
                    <nanosec>856000</nanosec>
                </initial_acknack_delay> -->
