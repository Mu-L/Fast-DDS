                </reliability>
                <durability>
                    <kind>VOLATILE</kind>
                </durability>
                <data_sharing>
                    <kind>OFF</kind>
                </data_sharing>
            </qos>
            <times>
                <initial_heartbeat_delay>
                    <sec>3</sec>
                </initial_heartbeat_delay>
                <heartbeat_period>
                    <sec>3</sec>
                </heartbeat_period>
                <nack_response_delay>
